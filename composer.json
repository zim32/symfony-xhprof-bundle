{
  "name": "zim32/xhprof-profiler-bundle",
  "description": "This bundle add additional XHProf menu item in symfony debug toolbar",
  "type": "symfony-bundle",
  "keywords": ["symfony", "bundle", "xhprof", "debug"],
  "license": "MIT",
  "homepage": "https://github.com/zim32/symfony-xhprof-bundle",
  "authors": [
    {
      "name": "zim32",
      "email": "yurij.uvarov@gmail.com"
    }
  ],
  "support": {
    "email": "yurij.uvarov@gmail.com",
    "issues": "https://github.com/zim32/symfony-xhprof-bundle/issues"
  },
  "autoload": {
    "psr-4": {
      "Zim\\XhProfBundle\\": ""
    }
  },
  "require": {
    "php": "^7.1|^8.0",
    "symfony/framework-bundle": "^4.0|^5.0|^6.0"
  },
  "suggest" : {
    "ext-tideways_xhprof" : "This package requires tideways_xhprof extension. Activate it for FPM only"
  }
